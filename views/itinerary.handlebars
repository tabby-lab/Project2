<div class="container-two">
    <div class="row">
        {{#each data}}
        <div class="col s4" >

            <div class="card" style="height:50vh">
                <div class="card-image waves-effect waves-block waves-light">
                   
                </div>
                <div class="card-content">
                    <span class="card-title activator grey-text text-darken-4">{{this.id}}<i
                            class="material-icons right">more_vert</i></span>
                    <p>{{this.hotel_info}}</p>
                    {{#each this.itinerary}}
                    <p>{{this}}</p>
                    {{/each}}

                    
                </div>
                
            </div>


        </div>
        {{/each}}
    </div>

   </div>


   <div class="footer">
  <p>Copyright  &copy; Project 2</p>
</div>
   

